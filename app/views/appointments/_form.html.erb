
  <%= form_for @appointment do |f| %>
    <div><%= f.label "title: " %><%= f.text_field :title %></div><br>
    <div><%= f.label "description: " %><%= f.text_field :description %></div><br>
    <div><%= f.label "date: " %><%= f.date_field :date %></div><br>
    <div><%= f.label "time: " %><%= f.time_field :time %></div><br>
    <br>
    <div><%= f.submit%><br>
  <% end %>
